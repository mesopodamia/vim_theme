syntax on
autocmd FileType python set omnifunc=pythoncomplete#Complete  
autocmd FileType php set omnifunc=phpcomplete#Complete  
autocmd FileType sql set omnifunc=sqlcomplete#Complete  
set autoindent
set smartindent
set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
set smarttab
set relativenumber
set textwidth=0
set wrapmargin=2
set showmatch
set scrolloff=5
set laststatus=2
set incsearch
set hlsearch
set paste
set history=1000
